<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="oval" >
    <corners
        android:radius="14dp"
        />
    <gradient
        android:gradientRadius="10dp"
        android:endColor="@color/vk_light_color"
        android:startColor="#E8E8E8"
        android:type="radial"
               />
    <padding

        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        />

    <stroke
        android:width="2dp"
        android:color="@color/vk_black"
        />
</shape>